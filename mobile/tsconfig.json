{
  "extends": "expo/tsconfig.base.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/services/*": [
        "./services/*"
      ],
      "@/stores/*": [
        "./stores/*"
      ],
      "@/utils/*": [
        "./utils/*"
      ],
      "@/constants/*": [
        "./constants/*"
      ],
      "@/types/*": [
        "./types/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}